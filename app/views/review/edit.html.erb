<%= form_for(:review, url: review_path, method: :put) do |f| %>

  <div class="field">
    <%= f.label :instructor_name %><br />
    <%= f.text_field :instructor_name, autocomplete: "new-instructor" %>
  </div>
  
  <div class="field">
    <%= f.label :review %><br />
    <%= f.text_field :body, autocomplete: "new-review" %>
  </div>

  <div class="actions">
    <%= f.submit "Update review" %>
  </div>
<% end %>